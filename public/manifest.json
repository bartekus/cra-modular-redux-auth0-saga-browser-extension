{
  "name": "CRA Extension",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "CRA Extension with redux-saga & Auth0 authentication",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "identity",
    "notifications"
  ]
}