{
  "name": "cra-modular-redux-auth0-saga-browser-extension",
  "homepage": "/",
  "version": "0.1.3",
  "private": true,
  "license": "MIT",
  "author": "Bartek Kus <bartekus@gmail.com> (https://bartekus.com/)",
